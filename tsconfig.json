{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false
    },
    "filesGlob": [
      "main.ts",
      "typings.d.ts",
      "login_register/**/*.ts",
      "!login_register/**/*.spec.ts",
      "demoApp/**/*.ts",
      "!demoApp/**/*.spec.ts"
    ]
}
